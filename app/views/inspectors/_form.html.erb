<%= simple_form_for @inspector do |f| %>
<%= f.error_notification %>
<%= f.input :last_name, placeholder: t(:last_name), label: t(:last_name) %>
<%= f.input :first_name, placeholder: t(:first_name), label: t(:first_name) %>
<%= f.input :middle_name, placeholder: t(:middle_name), label: t(:middle_name) %>
<%= f.input :position, placeholder: t(:position), label: t(:position) %>
<%= f.button :submit, t(:save), class: "btn btn-outline-inverse" %>
<% end %>
