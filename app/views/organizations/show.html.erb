<p id="notice"><%= notice %></p>
<div class="well well-sm">
  <p>
    <strong><%= t(:company_name)%>:
    </strong>
    <%= @organization.company_name %>
  </p>

  <p>
    <strong><%= t(:first_name) %>:</strong>
    <%= @organization.first_name %>
  </p>
  <p>
    <strong><%= t(:last_name)%>:</strong>
    <%= @organization.last_name %>
  </p>
  <p>
    <strong><%= t(:middle_name) %>:</strong>
    <%= @organization.middle_name %>
  </p>
  <p>
    <strong><%= t(:position) %>:</strong>
    <%= @organization.position %>
  </p>
</div>
<% if current_user.is_admin == true %>
  <%= link_to t(:edit), edit_organization_path(@organization) %>
  |
<% end %>
<%= link_to t(:back), organizations_path %>
