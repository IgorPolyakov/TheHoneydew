<%= simple_form_for @organization, url: organizations_path, as: 'organization' do |f| %>
<%= f.error_notification %>
<%= f.input :company_name, placeholder: t(:company_name) %>
<%= f.input :first_name, placeholder: t(:first_name) %>
<%= f.input :last_name, placeholder: t(:last_name) %>
<%= f.input :middle_name, placeholder: t(:middle_name) %>
<%= f.input :position, placeholder: t(:position) %>
<%= f.button :submit %>
<% end %>
