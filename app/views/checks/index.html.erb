<p id="notice"><%= notice %></p>

<h1>Checks</h1>

<table class="table">
  <thead>
    <tr>
      <th>Count</th>
      <th>Name</th>
      <th>Deadline</th>
      <th>Object</th>
      <th>Category</th>
      <th>Tom</th>
      <th>Dsp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checks.each do |check| %>
      <tr>
        <td><%= check.count %></td>
        <td><%= check.name %></td>
        <td><%= check.deadline %></td>
        <td><%= check.object %></td>
        <td><%= check.category %></td>
        <td><%= check.tom %></td>
        <td><%= check.dsp %></td>
        <td><%= link_to 'Show', check %></td>
        <td><%= link_to 'Edit', edit_check_path(check) %></td>
        <td><%= link_to 'Destroy', check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Check', new_check_path %>
