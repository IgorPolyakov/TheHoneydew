<%= simple_form_for @check, url: checks_path, as: 'check' do |f| %>
<%= f.error_notification %>
<%= f.input :amount, placeholder: t(:amount), label: t(:amount) %>
<%= f.input :checks_number, placeholder: t(:checks_number), label: t(:checks_number) %>
<%= f.input :deadline, label: t(:deadline) %>
<%= f.input :inspector_id, label: t(:inspector), collection: @inspectors %>
<%= f.input :reason, label: t(:reason), as: :check_boxes, collection: @reason, include_blank: false, include_hidden: false %>
<%= f.input :response_measure,label: t(:response_measure), as: :check_boxes, collection: @response_measure, include_blank: false, include_hidden: false  %>
<%= f.input :violation,label: t(:violation), as: :check_boxes, collection: @violation, include_blank: false, include_hidden: false  %>
<%= f.input :category,label: t(:category), as: :check_boxes, collection: @category, include_blank: false, include_hidden: false %>
<%= f.input :direction,label: t(:direction), as: :check_boxes, collection: @direction, include_blank: false, include_hidden: false %>
<%= f.input :term, placeholder: t(:term), label: t(:term) %>
<%= f.input :case_number, placeholder: t(:case_number), label: t(:case_number) %>
<%= f.input :number_of_volumes, placeholder: t(:number_of_volumes), label: t(:number_of_volumes) %>
<%= f.input :executive, placeholder: t(:executive), label: t(:executive) %>
<%= f.input :for_office, label: t(:for_office) %>
<%= f.input :controll, label: t(:controll) %>
<%= f.button :submit %>
<% end %>
