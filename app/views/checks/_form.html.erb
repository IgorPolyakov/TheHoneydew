<%= simple_form_for @check, url: checks_path, as: 'check' do |f| %>
<%= f.error_notification %>
<%= f.input :amount, placeholder: t(:amount) %>
<%= f.input :checks_number, placeholder: t(:checks_number) %>
<%= f.input :deadline %>
<%= f.input :inspector_id, collection: @inspectors %>
<%= f.input :reason, as: :check_boxes, collection: [t(:the_qa_and_sar_work_plan), t(:gto_commission), t(:zgto_commission),t(:appeal_of_applicant_organization),t(:other)]%>
<%= f.input :response_measure, as: :check_boxes, collection: [ t(:determination_of_contracts), t(:disciplinary_action), t(:repayment_of_funds), t(:procuracy), t(:eogv), t(:control_committee), t(:false_facts), t(:others) ] %>
<%= f.input :violation, as: :check_boxes, collection: [t(:labor_legislation), t(:legislation_on_state_and_municipal_services), t(:legislation_on_the_contract_system), t(:budget_legislation), t(:departmental_legislation), t(:others)] %>
<%= f.input :category, as: :check_boxes, collection: [t(:performance), t(:preservation_and_use_of_property), t(:state_municipal_services), t(:contract_system), t(:anti_corruption), t(:for_calls_inappropriate_use_of_budgetary_funds),
t(:challenging_the_actions_of_officials), t(:others)] %>
<%= f.input :direction, as: :check_boxes, collection: [t(:for_information), t(:for_accept), t(:for_removal)] %>
<%= f.input :term, placeholder: t(:term) %>
<%= f.input :case_number, placeholder: t(:case_number) %>
<%= f.input :number_of_volumes, placeholder: t(:number_of_volumes) %>
<%= f.input :executive, placeholder: t(:executive) %>
<%= f.input :for_office %>
<%= f.button :submit %>
<% end %>
